Include ~/.orbstack/ssh/config

# Job account
Host github.com
   HostName github.com
   IdentityFile ~/.ssh/job
   IdentitiesOnly yes
   
# Dev account
Host github-dev
   HostName github.com
   IdentityFile ~/.ssh/id_ed25519
   IdentitiesOnly yes

# Job gitlab
Host gitlab.com
   HostName gitlab.com
   IdentityFile ~/.ssh/gitlab
   IdentitiesOnly yes